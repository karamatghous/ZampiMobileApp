{
  "name": "zampi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android-dev": "set ENVFILE=.env.dev && npx react-native run-android --no-jetifier --variant=devDebug --appId=com.zampi.app.dev",
    "clean-android": "cd android && gradlew clean && cd.. ",
    "clean-build": "rm -r android/app/src/main/assets/index.android.bundle",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-build-dev": "export ENVFILE=.env.dev && cd android && gradlew clean && gradlew assembleRelease",
    "android-build-dev-ios": "export ENVFILE=.env.dev && cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android-build-dev-ios-debug": "export ENVFILE=.env.dev && cd android && ./gradlew clean && ./gradlew assembledevDebug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
